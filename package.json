{
  "name": "edgeneuro-workspace",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"No tests yet\" && exit 0",
    "deploy:router": "cd synapse_core && npx wrangler deploy",
    "deploy:hr": "cd test-agents/hr-agent && npx wrangler deploy",
    "deploy:it": "cd test-agents/it-agent && npx wrangler deploy",
    "deploy:all": "npm run deploy:router && npm run deploy:hr && npm run deploy:it",
    "dev:router": "cd synapse_core && npx wrangler dev",
    "setup:poc": "bash scripts/poc-setup.sh"
  },
  "devDependencies": {
    "wrangler": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  }
}
